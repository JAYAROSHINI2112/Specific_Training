#include <stdio.h>

int main()
{
    int mat[20][20],i,j,row,col,max;
    
    scanf("%d%d", &row, &col);
    for(i = 0; i < row; i ++)
    {
        for(j = 0; j < col; j ++)
         scanf("%d",&mat[i][j]);
    }
    
        for(i = 0; i < row; i ++)
    {
        max = mat[i][j];
        for(j = 0; j < col; j ++)
        {
          if(max < mat[i][j])
            max = mat[i][j];
        }
        printf("%d ",max);
    }
    
}
