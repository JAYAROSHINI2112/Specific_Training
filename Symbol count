
#include <stdio.h>
int main()
{
  int n,count = 0,i;
  char str[1000];
  scanf("%s",str);
  for(i = 0;str[i]!= '\0';i++)
  {
      if(str[i]>='A' && str[i]<='Z')
       continue;
      else if(str[i]>='a' && str[i]<='z')
       continue;
      else if(str[i]>='0' && str[i]<='9') 
       continue;
      else
       count++;
  }
  printf("%d",count);
}
